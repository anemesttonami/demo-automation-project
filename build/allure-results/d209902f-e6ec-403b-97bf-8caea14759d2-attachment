<script>
    $(function(){
        $("body").on("change", ".exam-filter select[name='scheme']", function(event) {
            var levelSelect = $(".exam-filter select[name='level']");

            if ($(this).val() == '') {
                var options = $(this).find('option');
                schemes = $.map(options ,function(option) {
                    return option.value;
                });
            } else {
                var schemes = [$(this).val()];
            }

            $.ajax({
                type: "POST",
                url: "/ajax_local_exam",
                data: {
                    action: 'getLevel',
                    value: schemes
                },
                success: function(result) {
                    res = $.parseJSON(result);
                    if(res['status'] === 1) {
                        var levelOptions = res['data']['level'];
                        levelSelect.find('option').not(':first').remove().end();
                        for (const [key, value] of Object.entries(levelOptions)) {
                            levelSelect.append("<option value='"+value+"'>"+value+"</option>");
                        }
                    }
                }
            });
        });

        $("body").on("change", ".exam-filter select[name='country']", function(event) {
            var citySelect = $(".exam-filter select[name='city']");
            $.ajax({
                type: "POST",
                url: "/ajax_local_exam",
                data: {
                    action: 'getCities',
                    value: $(this).val()
                },
                success: function(result) {
                    res = $.parseJSON(result);
                    if(res['status'] === 1) {
                        var cityOptions = res['data']['cities'];
                        citySelect.find('option').not(':first').remove().end();
                        for (const [key, value] of Object.entries(cityOptions)) {
                            citySelect.append("<option value='"+key+"'>"+value+"</option>");
                        }
                    }
                }
            });
        });

        $("body").on("click", ".exam-filter .toggle-filter-pane", function(event) {
            $('.filter-body').toggleClass('hide');
        });
    });
</script>
    
</div></div>
